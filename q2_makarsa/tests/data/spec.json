 "scales": [
  {
   "name": "color",
   "type": "ordinal",
   "domain": {
    "data": "node-data",
    "field": {
     "signal": "colorSelect"
    },
    "sort": true
   },
   "range": {
    "scheme": "category20c"
   }
  },
  {
   "name": "sizeScale",
   "type": "pow",
   "exponent": 2,
   "domain": {
    "data": "node-data",
    "field": {
     "signal": "sizeSelect"
    }
   },
   "range": [
    8,
    {
     "signal": "2 * nodeRadius * nodeRadius"
    }
   ]
  },
  {
   "name": "weight",
   "type": "linear",
   "domain": {
    "data": "link-data",
    "field": "weight"
   },
   "range": [
    0.5,
    {
     "signal": "linkWidth"
    }
   ]
  }
 ],
 "legends": [
  {
   "fill": "color",
   "labelFontSize": {
    "value": 12
   },
   "labelLimit": {
    "value": 0
   },
   "encode": {
    "labels": {
     "update": {
      "opacity": [
       {
        "test": "colorSelect === 'None'",
        "value": 0
       },
       {
        "value": 1
       }
      ]
     }
    },
    "symbols": {
     "update": {
      "opacity": [
       {
        "test": "colorSelect === 'None'",
        "value": 0
       },
       {
        "value": 1
       }
      ]
     }
    }
   }
  }
 ],
 "marks": [
  {
   "name": "nodes",
   "type": "symbol",
   "zindex": 1,
   "from": {
    "data": "node-data"
   },
   "on": [
    {
     "trigger": "fix",
     "modify": "node",
     "values": "fix === true ? {fx: node.x, fy: node.y} : {fx: fix[0], fy: fix[1]}"
    },
    {
     "trigger": "!fix",
     "modify": "node",
     "values": "{fx: null, fy: null}"
    }
   ],
   "encode": {
    "enter": {
     "size": [
      {
       "test": "sizeSelect === 'None'",
       "signal": "2 * nodeRadius * nodeRadius"
      },
      {
       "scale": "sizeScale",
       "field": {
        "signal": "sizeSelect"
       }
      }
     ],
     "fill": {
      "scale": "color",
      "field": {
       "signal": "colorSelect"
      }
     },
     "stroke": {
      "value": "white"
     }
    },
    "update": {
     "size": [
      {
       "test": "sizeSelect === 'None'",
       "signal": "2 * nodeRadius * nodeRadius"
      },
      {
       "scale": "sizeScale",
       "field": {
        "signal": "sizeSelect"
       }
      }
     ],
     "cursor": {
      "value": "pointer"
     },
     "fill": [
      {
       "test": "datum.index === active",
       "value": "#999"
      },
      {
       "scale": "color",
       "field": {
        "signal": "colorSelect"
       }
      }
     ]
    }
   },
   "transform": [
    {
     "type": "force",
     "iterations": 200,
     "restart": {
      "signal": "restart"
     },
     "static": {
      "signal": "static"
     },
     "signal": "force",
     "forces": [
      {
       "force": "center",
       "x": {
        "signal": "cx"
       },
       "y": {
        "signal": "cy"
       }
      },
      {
       "force": "collide",
       "radius": {
        "expr": "sqrt(datum.size)/2 + 0*nodeRadius + 0*(sizeSelect === '')"
       }
      },
      {
       "force": "nbody",
       "strength": {
        "signal": "nodeCharge"
       }
      },
      {
       "force": "link",
       "links": "link-data",
       "distance": {
        "signal": "linkDistance"
       }
      },
      {
       "force": "link",
       "links": "link-data",
       "strength": {
        "signal": "linkStrength"
       }
      }
     ]
    }
   ]
  },
  {
   "type": "path",
   "from": {
    "data": "link-data"
   },
   "interactive": false,
   "encode": {
    "update": {
     "stroke": [
      {
       "test": "active != null && (datum.source.index == active || datum.target.index == active)",
       "value": "#999"
      },
      {
       "value": "#ccc"
      }
     ],
     "strokeWidth": {
      "scale": "weight",
      "field": "weight"
     }
    }
   },
   "transform": [
    {
     "type": "linkpath",
     "require": {
      "signal": "force"
     },
     "shape": "line",
     "sourceX": "datum.source.x",
     "sourceY": "datum.source.y",
     "targetX": "datum.target.x",
     "targetY": "datum.target.y"
    }
   ]
  }
 ]
}