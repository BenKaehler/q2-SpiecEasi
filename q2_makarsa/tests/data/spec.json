{
 "$schema": "https://vega.github.io/schema/vega/v5.json",
 "description": "A node-link diagram with force-directed layout.",
 "width": 700,
 "height": 500,
 "padding": 0,
 "autosize": {
  "type": "pad",
  "resize": true
 },
 "signals": [
  {
   "name": "colorSelect",
   "value": "None",
   "bind": {
    "input": "select",
    "name": "color ",
    "options": [
     "None"
    ]
   }
  },
  {
   "name": "sizeSelect",
   "value": "None",
   "bind": {
    "input": "select",
    "name": "size ",
    "options": [
     "None"
    ]
   }
  },
  {
   "name": "cx",
   "update": "width / 2"
  },
  {
   "name": "cy",
   "update": "height / 2"
  },
  {
   "name": "nodeRadius",
   "value": 8,
   "bind": {
    "input": "range",
    "min": 1,
    "max": 30,
    "step": 1,
    "name": "node scale"
   }
  },
  {
   "name": "linkWidth",
   "value": 5,
   "bind": {
    "input": "range",
    "min": 0.5,
    "max": 10,
    "step": 0.25,
    "name": "link scale"
   }
  },
  {
   "name": "nodeCharge",
   "value": -30,
   "bind": {
    "input": "range",
    "min": -60,
    "max": 10,
    "step": 1,
    "name": "node charge"
   }
  },
  {
   "name": "linkDistance",
   "value": 30,
   "bind": {
    "input": "range",
    "min": 5,
    "max": 100,
    "step": 1,
    "name": "link distance"
   }
  },
  {
   "name": "linkStrength",
   "value": 0,
   "bind": {
    "input": "range",
    "min": 0,
    "max": 2,
    "step": 0.1,
    "name": "link strength"
   }
  },
  {
   "name": "static",
   "value": true,
   "bind": {
    "input": "checkbox",
    "name": "static "
   }
  },
  {
   "description": "State variable for active node fix status.",
   "name": "fix",
   "value": false,
   "on": [
    {
     "events": "symbol:mouseout[!event.buttons], window:mouseup",
     "update": "false"
    },
    {
     "events": "symbol:mouseover",
     "update": "fix || true"
    },
    {
     "events": "[symbol:mousedown, window:mouseup] > window:mousemove!",
     "update": "xy()",
     "force": true
    }
   ]
  },
  {
   "description": "Graph node most recently interacted with.",
   "name": "node",
   "value": null,
   "on": [
    {
     "events": "symbol:mouseover",
     "update": "fix === true ? item() : node"
    }
   ]
  },
  {
   "description": "Flag to restart Force simulation upon data changes.",
   "name": "restart",
   "value": false,
   "on": [
    {
     "events": {
      "signal": "fix"
     },
     "update": "fix && fix.length"
    }
   ]
  },
  {
   "description": "Flag to highlight node and links",
   "name": "active",
   "value": null,
   "on": [
    {
     "events": "symbol:mouseover",
     "update": "datum.index"
    },
    {
     "events": "mouseover[!event.item]",
     "update": "null"
    }
   ]
  },
  {
   "description": "Selection for node table",
   "name": "nodeSelect",
   "value": null,
   "on": [
    {
     "events": "symbol:mousedown",
     "update": "item()"
    }
   ]
  }
 ],
